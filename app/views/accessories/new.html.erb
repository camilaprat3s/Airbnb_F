<h1>Accessories#new</h1>
<%= form_for @accessory, url: accessories_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.number_field :price, step: 0.01 %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
